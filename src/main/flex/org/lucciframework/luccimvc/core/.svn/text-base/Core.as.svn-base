package org.lucciframework.luccimvc.core
{
	/**
	 * Cette classe est héritée par toutes les autres classes
	 * elle permet de référencer la façade associé à l'instance 
	 * de l'objet courant
	 */
	public class Core implements ICore
	{
		
		protected var _facadeRef : String;
		
		/**
		 * GET la référence de la façade
		 */
		public function get facadeRef () : String {
			return _facadeRef;
		}
		
		/**
		 * SET la référence de la façade
		 */
		public function set facadeRef ( value : String ) : void {
			_facadeRef = value;
		}
	}
}